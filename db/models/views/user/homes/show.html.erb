<%= render 'user/header' %>
<section class="content">
  <%= @profile.first_name %>
  <% unless @user.microposts.empty? %>
    <table class="microposts" summary="User microposts">
      <%= render @microposts %>
    </table>
    <%= will_paginate @microposts %>
    <% end %>
</section>
<%= render 'layouts/footer'%>